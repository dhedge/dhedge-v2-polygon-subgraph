    "build-prod": "graph build ./subgraph-prod.yaml",
    "deploy-dev": "graph deploy ./subgraph-dev.yaml --node https://api.thegraph.com/deploy/ --ipfs https://api.thegraph.com/ipfs/ dhedge/dhedge-v2-polygon-dev",
